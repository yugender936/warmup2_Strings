public String twoChar(String str, int index) {
  if(index < 1 || index >= str.length()-1){
    return ""+str.charAt(0)+str.charAt(1);
  }
  String str1 = "";
  int i = index;
  int count = 0;
  while(i < str.length() && count < 2){
    str1 = str1 + str.charAt(i);
    i++;
    count++;
  }
  return str1;
}
