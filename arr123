public boolean array123(int[] nums) {
  int[] arr = new int[3];
 for(int i = 0 ; i < nums.length ; i++){
    if(nums[i] == 1){
      int k = i;
      for(int j = 0 ; j <= 2 ; j++){
        if(k < nums.length){
          arr[j] = nums[k];
          k++;
        }
      }
    }
    if(arr[0] == 1 && arr[1] == 2 && arr[2] == 3){
      return true;
    }
  }
  return false;
}
