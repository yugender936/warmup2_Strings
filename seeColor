public String seeColor(String str) {
  if(str.length() < 3){
    return "";
  }
  String str1 = "";
  String str2 = "";
  for(int i = 0 ; i < 3 ; i++){
    str1 = str1 + str.charAt(i);
  }
  if(str.length() > 3){
    for(int i = 0 ; i < 4 ; i++){
      str2 = str2 + str.charAt(i);
    }
  }
  if(str1.equals("red")){
    return "red";
  }
  if(str2.equals("blue")){
    return "blue";
  }
  return "";
}
